/* pages/mine/company/teamCoachDetail/teamCoachDetail.wxss */
.teamCoachDetail-head{
  width: 100%;
  height:182rpx;
  position: relative;
}
.teamCoachDetail-head-bg,.teamCoachDetail-head-msg{
  width: 100%;
  height: 100%;
  position: absolute;
  top:0;
  left:0;
}
.teamCoachDetail-head-msg{
  padding:49rpx 37rpx;
  color:#fff;
  font-size:28rpx;
}
.teamCoachDetail-head-msg-name{
  font-size:36rpx;
  font-weight: bold;
  margin-bottom: 15rpx;
}
.teamCoachDetail-list{
  margin-top: 28rpx;
  height:169rpx;
  line-height: 169rpx;
  padding:0 37rpx;
}
.teamCoachDetail-list-img{
  width:98rpx;
  height:98rpx;
  border-radius: 50%;
  vertical-align: middle;
  margin-right: 31rpx;
  background: #eee;
}
.teamCoachDetail-list-name{
  font-size: 34rpx;
  font-weight: bold;
  margin-right: 62rpx;
}
.teamCoachDetail-list-percent{
  font-size:24rpx;
  color:#999;
  margin-right: 91rpx;
}
.teamCoachDetail-list-percent>text{
  font-size: 28rpx;
}
.teamCoachDetail-grade{
  font-size:28rpx;
}