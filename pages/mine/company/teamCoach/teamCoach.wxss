/* pages/mine/company/teamCoach/teamCoach.wxss */
.teamCoach-body{
  padding:59rpx 45rpx;
}
.teamCoach-list:not(:last-child){
  border-bottom:1px solid #eee;
  margin-bottom: 44rpx;
}
.teamCoach-list-msg{
  font-size:32rpx;
  font-weight: bold;
  display: flex;
  justify-content: space-between;
  margin-bottom: 24rpx;
}
.teamCoach-dot{
  width: 12rpx;
  height:12rpx;
  vertical-align: middle;
  background: #D3D1D1;
  border-radius: 40%;
  margin: 0 17rpx 5rpx 0;
}
.teamCoach-dot-act{
  background: #F56A32;
}
.teamCoach-go{
  font-size: 24rpx;
  color:#999;
}
.teamCoach-go>text{
  vertical-align: middle;
}
.teamCoach-go-img{
  width: 19rpx;
  height:19rpx;
  vertical-align: middle;
  margin-left: 9rpx;
}
.teamCoach-list-img{
  margin-bottom: 32rpx;
  padding-left: 25rpx;
}
.teamCoach-user-img{
  width: 52rpx;
  height:52rpx;
  vertical-align: middle;
  margin-right: 8rpx;
  border-radius: 50%;
  background: #eee;
}