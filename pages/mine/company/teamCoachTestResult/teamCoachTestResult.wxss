/* pages/mine/company/teamCoachTestResult/teamCoachTestResult.wxss */
.testCard-head{
  color:#fff;
  width: 100%;
  height:301rpx;
  position: relative;
  margin-bottom: 35rpx;
}
.testCard-head-bg,.testCard-head-msg{
  width: 100%;
  height:100%;
  position: absolute;
  top:0;
  left:0;
}
.testCard-head-msg{
  padding:54rpx 41rpx;
}
.testCard-head-msg-name{
  display: flex;
  justify-content: space-between;
  font-size: 42rpx;
  font-weight:bold;
}
.testCard-go-repair{
  width:137rpx;
  height:54rpx;
  border:1px solid rgba(255,255,255,1);
  opacity:0.55;
  border-radius:4rpx;
  font-size: 26rpx;
  line-height: 54rpx;
  text-align: center
}
.testCard-go-repair>image{
  width: 24rpx;
  height:24rpx;
  vertical-align: middle;
  margin:0 0 10rpx 10rpx;
}
.testCard-head-footer{
  display: flex;
  justify-content: space-between
}
.testCard-head-msg-num{
  margin-top:50rpx;
}
.testCard-head-msg-num>view{
  font-size: 24rpx;
  opacity: 0.7;
  text-align: center;
}
.testCard-head-msg-num>view:not(:last-child){
  margin-right: 64rpx;
}
.testCard-head-num-list{
  font-size: 36rpx;
  opacity: 0.8;
  margin-top: 19rpx;
}
.testCard-right-probability{
  width:123rpx;
  height:123rpx;
  background:rgba(255,255,255,1);
  box-shadow:0px 1px 2px 0px rgba(3,0,0,0.05);
  border-radius:50%;
  font-size: 22rpx;
  margin-top: 21rpx;
  text-align: center;
  color:#FBC9BB;
}
.testCard-right-probability>view:nth-child(1){
  font-size: 31rpx;
  margin-top: 23rpx;
}
.coach-model{
  padding:44rpx 40rpx;
  margin-bottom: 26rpx;
}
.testCard-list-name{
  font-size:30rpx;
  color:#333;
  font-weight:bold;
  border-bottom:1px solid #eee;
  padding-bottom:35rpx;
  margin-bottom: 29rpx;
}
.testCard-list-num>view{
  width: 95%;
  vertical-align: middle;
}
.testCard-list-num-btn{
  width:26rpx;
  height:26rpx;
  vertical-align: middle;
}
.testCard-list-num>view>view{
  width:46rpx;
  height:46rpx;
  border-radius:50%;
  margin-right: 32rpx;
  font-size: 26rpx;
  color:#fff;
  background: #EEEEEE;
  line-height: 46rpx;
  text-align: center;
}
.right{
  background: #44CD98!important;
}
.wrong{
  background: #F56A32!important;
}