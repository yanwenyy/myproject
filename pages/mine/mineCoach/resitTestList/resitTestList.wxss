/* pages/mine/mineCoach/resitTestList/resitTestList.wxss */
.resit-ul{
  margin-bottom: 200rpx;
}
.resit-name{
  font-size: 32rpx;
  color:#333;
  margin-bottom: 41rpx;
  font-weight: bold;
}
.coach-model{
  padding:44rpx 40rpx;
}
.resit-li{
  margin-bottom: 22rpx;
}
.resit-li-title{
  font-size: 30rpx;
  line-height: 48rpx;
  padding-bottom:45rpx;
  border-bottom:1px solid #eee;
  margin-bottom: 38rpx;
}
.resit-li-num>view{
  width: 54rpx;
  height:54rpx;
  line-height: 54rpx;
  text-align: center;
  border-radius: 50%;
  margin-right: 31rpx;
  background:#EEEEEE;
  color:#fff;
}
.resit-li-num-already{
  background:#D2D5DC!important;
}
.resit-li-footer{
  margin-top:35rpx;
  text-align: right;
}
.resit-li-btn{
  width:154rpx;
  height:58rpx;
  line-height:58rpx;
  background:rgba(245,106,50,1);
  border-radius:29rpx;
  text-align: center;
  font-size:30rpx;
  color:#fff;
}
.resit-li-btn-repeat{
  background: #FA9972!important;
}
.sub-btn{
  width:100%;
  height:170rpx;
  background:#fff;
  padding:26rpx 32rpx;
  position: fixed;
  bottom:0;
  left:0;
}
.sub-btn>button{
  width:690rpx;
  height:88rpx;
  background:rgba(245,106,50,1);
  border-radius:10rpx;
  font-size:32rpx;
  color:#fff;
  border:none;
}