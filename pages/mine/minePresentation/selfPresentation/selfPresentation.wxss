/* pages/mine/minePresentation/selfPresentation/selfPresentation.wxss */
.index-swiper-img{
  width: 100%;
  height:100%;
}
.selfPresentation-banner{
  width: 100%;
  height:238rpx;
  border-radius: 16rpx;
  margin-bottom: 22rpx;
}
.selfPresentation-body{
  background: #fff;
  padding:59rpx 45rpx 0 45rpx;
}
.selfPresentation-list-dot{
  width: 12rpx;
  height:12rpx;
  background: #DCDCDC;
  border-radius: 50%;
  vertical-align: top;
  margin-right: 17rpx;
  margin-top: 15rpx;
}
.selfPresentation-list-dot-act{
  background: #F56A32;
}
.selfPresentation-list-msg{
  width: 95%;
}
.selfPresentation-list-date{
  font-size: 24rpx;
  display: flex;
  justify-content: space-between;
  margin-bottom: 15rpx;
}
.selfPresentation-list-date>.orange{
  margin-top: 8rpx;
}
.selfPresentation-list-date>text:nth-child(1){
  font-size: 32rpx;
  font-weight: bold;
}
.selfPresentation-list-title{
  font-size:28rpx;
  color:#999;
  border-bottom:1px solid #eee;
  padding-bottom:35rpx;
  margin-bottom: 45rpx;
  display: flex;
  justify-content: space-between;
}
.selfPresentation-list-btn{
  font-size: 24rpx;
  color:#999;
}
.selfPresentation-list-btn>image{
  width:19rpx;
  height:19rpx;
  margin-left: 8rpx;
  vertical-align: middle;
}