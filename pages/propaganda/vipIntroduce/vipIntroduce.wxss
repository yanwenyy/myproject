/* pages/propaganda/vipIntroduce/vipIntroduce.wxss */
.vipIntroduce-head{
  width: 100%;
  height:336rpx;
}
.vipIntroduce-body{
  padding:0 34rpx;
}
.vipIntroduce-title{
  text-align: center;
  margin-bottom: 46rpx;
}
.vipIntroduce-title>image{
  width: 635rpx;
  height:34rpx;
}
.vipIntroduce-model{
  box-sizing: border-box;
  padding:40rpx 0 57rpx 0;
  box-shadow:0px 1rpx 18rpx 0px rgba(4,0,0,0.04);
  border-radius:16rpx;
  margin-bottom: 18rpx;
}
.vipIntroduce-title{
  text-align: center;
  color:#333;
  font-size: 30rpx;
  font-weight: bold;
  margin-bottom: 66rpx;
}
.vipIntroduce-zcjd{
  padding:0 45rpx;
  display: flex;
  justify-content: space-between;
}
.vipIntroduce-zcjd>view{
  font-size:26rpx;
  color:#666;
  text-align: center;
}
.vipIntroduce-zcjd-img{
  width: 76rpx;
  height:76rpx;
  margin-bottom: 16rpx;
}
.vipIntroduce-sqzh{
  padding: 0 103rpx;
  display: flex;
  justify-content: space-between;
}
.vipIntroduce-sqzh>view{
  font-size:26rpx;
  color:#666;
  text-align: center;
}
.vipIntroduce-sqzh-img{
  width: 116rpx;
  height:116rpx;
  margin-bottom: 7rpx;
}
.vipIntroduce-hyfl{
  padding-left: 62rpx;
}
.vipIntroduce-hyfl-list:not(:last-child){
  margin-bottom: 65rpx;
}
.vipIntroduce-hyfl-img{
  width: 76rpx;
  height:76rpx;
  vertical-align: top;
  margin-right: 18rpx;
}
.vipIntroduce-hyfl-list>view{
  vertical-align: top;
}
.vipIntroduce-hyfl-list-title{
  font-size:28rpx;
  color:#333;
  margin-bottom: 8rpx;  
}
.vipIntroduce-hyfl-list-msg{
  font-size:24rpx;
  color:#999;
}
.vipIntroduce-date{
  padding:0 39rpx;
}
.vipIntroduce-date-list{
  font-size: 28rpx;
  color:#999;
}
.vipIntroduce-date-list:not(:last-child){
  margin-bottom: 44rpx;
}
.vipIntroduce-date-list-title{
  color:#333;
  font-size:30rpx;
  margin-bottom: 16rpx;
}