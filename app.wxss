.container {
  width: 100%;
  min-height: 100vh;
  height:auto;
  padding:32rpx 34rpx;
  background: #F4F7FD;
  box-sizing: border-box;
}
.shadow{
  width:100%;
  height:100%;
  background: rgba(0,0,0,0.3);
  position: fixed;
  top:0;
}
.inline-block{
  display: inline-block;
}
.box-sizing{
  box-sizing: border-box
}
.orange{
  color:#FA743E!important;
}
.coach-model{
  width: 100%;
  height:auto;
  background: #fff;
  border-radius: 16rpx;
}